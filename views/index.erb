<h1>질문하기</h1>
<form action="/ask">
  name :
  <select name="name">
    <option value="익명">익명</option>
    <option value="<%= session[:email] %>">내 아이디</option>
  </select><br>

  question :
  <input type="text" name="question"><br>
  <input type="submit">
</form>
<% @questions.each do |q| %>
  <p>질문자 : <%= q.name %></p>
  <p>질문 : <%= q.content %></p>
<% end %>
